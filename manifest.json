{
  "manifest_version": 2,
  "name": "fseconomy Assignment Finder",
  "description": "Plugin to help search for assignments on the fseconomy website",
  "version": "1.1.0",
  "permissions": [
    "activeTab"
  ],
  "page_action": {
    "default_icon": "icons/icon-32.png",
    "default_title": "Assignment Finder",
    "default_popup": "popup/popup.html",
    "show_matches": [
      "*://server.fseconomy.net/*"
    ]
  },
  "icons": {
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  }
}
